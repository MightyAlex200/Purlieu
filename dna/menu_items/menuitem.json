{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "http://example.com/menu_items/menuitem.json",
  "type": "object",
  "required": [
    "age",
    "name"
  ],
  "properties": {
    "age": {
      "maximum": 120,
      "minimum": 0,
      "default": 0,
      "type": "integer"
    },
    "name": {
      "type": "string",
      "pattern": ".+"
    }
  }
}